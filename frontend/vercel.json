{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
  ,
  "env": {
    "REACT_APP_API_URL": "https://umeed-backend-ochb.onrender.com/api"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install"
}